{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "jsx": "react-jsx",
        "strict": true,
        "skipLibCheck": true,
        "allowJs": true,
        "noEmit": true,
        "isolatedModules": true,
        "verbatimModuleSyntax": false
    }
}