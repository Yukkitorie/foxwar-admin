{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler", // Или "Node"
    "jsx": "react-jsx", // Важно для Keystatic
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true
  }
}